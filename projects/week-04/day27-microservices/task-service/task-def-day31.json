{
    "family": "day31-task-service",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::294659522793:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "task-service",
            "image": "294659522793.dkr.ecr.ap-northeast-1.amazonaws.com/task-service:day31",
            "portMappings": [
                {
                    "containerPort": 8081,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/day31-task-service",
                    "awslogs-region": "ap-northeast-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ]
}